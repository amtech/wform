<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" packages="com.icss.base.log.log4j2.plugin">
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level - %msg %ex%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="console" />
		</Root>
		<Logger name="com.nway.wform" level="debug" additivity="false">
			<AppenderRef ref="console" />
		</Logger>
	</Loggers>
</Configuration>